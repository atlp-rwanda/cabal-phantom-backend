{
  "name": "phantom-cabal",
  "version": "1.0.0",
  "description": "transport-phantom",
  "main": "index.js",
  "scripts": {
    "migrate": "npx sequelize-cli db:migrate",
    "seeding": "npx sequelize-cli db:seed:all",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "migrateDB": "npx sequelize-cli db:migrate:undo:all && npm run migrate && npm run seeding",
    "build": "babel src --out-dir dist --source-maps inline --copy-files",
    "start": "npm run migrateDB && npm run build && node ./dist/app.js",
    "test": "NODE_ENV=test npm run migrate:reset && NODE_ENV=test npm run seeding && NODE_ENV=test nyc mocha --recursive --require @babel/polyfill --require @babel/register ./src/test/*.js --timeout 500000000 --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "nodemon --exec babel-node ./src/app.js"
  },
  "author": "Niyiragira",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "coveralls": "^3.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "i18n": "^0.13.2",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^8.1.3",
    "nodemailer": "^6.4.13",
    "nyc": "^15.1.0",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "sinon": "^9.0.3",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "eslint": "^7.9.0",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/cabal-phantom-backend.git"
  },
  "keywords": [
    "BUSES"
  ],
  "bugs": {
    "url": "https://github.com/atlp-rwanda/cabal-phantom-backend/issues"
  },
  "homepage": "https://github.com/atlp-rwanda/cabal-phantom-backend#readme"
}
